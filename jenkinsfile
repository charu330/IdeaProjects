pipeline {
     agent any

     stages {
     stage ('compile stage') {
          steps {
                    withMaven(maven:'Maven1') {
                    sh 'mvn clean compile'
                    }
          }
     }

     stage ('Testing Stage') {

              steps {
                                  withMaven(maven:'Maven1') {
                                  sh 'mvn test'
                                  }
                        }
     }


     }
}